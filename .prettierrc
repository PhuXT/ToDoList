{
  "overrides": [
    {
      "files": "*.js",
      "options": {
        "arrowParens": "always",
        "semi": true,
        "singleQuote": true,
        "trailingComma": "all",
        "bracketSpacing": false,
        "printWidth": 80,
        "tabWidth": 4
      }
    },
    {
      "files": "*.html",
      "options": {
        "tabWidth": 4
      }
    }
  ]
}
